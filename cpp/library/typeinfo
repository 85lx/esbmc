#ifndef STL_TYPEINFO
#define STL_TYPEINFO

#include "definitions.h"

struct typeinfo {
  virtual ~typeinfo();
  bool operator== (const typeinfo& rhs) const;
  bool operator!= (const typeinfo& rhs) const;
  bool before (const typeinfo& rhs) const;
  const char* name() const;
private:
  typeinfo (const typeinfo& rhs);
  typeinfo& operator= (const typeinfo& rhs);
};

#endif
