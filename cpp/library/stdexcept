#ifndef STL_STDEXCEPT
#define STL_STDEXCEPT

#include "definitions.h"
#include "exception"
#include "string"

namespace std {

struct runtime_error : public exception {
public:
  runtime_error();
  runtime_error(const char* what_arg) {
	  message = (char *)what_arg;
    __ESBMC_assert(0, "runtime_error");
  }
};

}

#endif
