#ifndef STL_STRING
#define STL_STRING

namespace std
{
	struct string
	{
		string();
		string(char[]);

		string& operator+(string&, string&);
	};
}

namespace std
{
	string& nondet_string();

	string& operator+(string& str1, string& str2)
	{
		return nondet_string();
	}

	inline ostream& operator << (ostream& o, string)
	{
		return o;
	}

}

#endif
